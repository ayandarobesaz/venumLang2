var x int
0 x write

while x load 100 < do
    x load 3 % 0 == x load 5 % 0 ==  && if
        "fizzbuzz" puts
    else
        x load 3 % 0 == if
            "fizz" puts
        else
            x load 5 % 0 == if
                "buzz" puts
            end
        end
    end
    // increment x 
    x load 1 + x write
end